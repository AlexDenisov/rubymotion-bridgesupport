-	profile_osx.a profile_ios.a
+	eprintf.a 10.4.a osx.a \
+	profile_osx.a
 #	Apple Internal: Disable building asan libraries. <rdar://12423266>
 #	asan_osx.a asan_osx_dynamic.dylib ubsan_osx.a
 endif
